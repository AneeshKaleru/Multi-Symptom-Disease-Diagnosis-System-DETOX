  <div class="diagnosis">
    <h2>
      Check Yourself Up!
    </h2>
    <form id="diagnosisForm" class="needs-validation" novalidate action="/diagnose" method="post">
      <div class="row g-3">

        <div class="col-12">
          <label for="name" class="form-label">Name</label>
          <div class="input-group has-validation">
            <input type="text" class="form-control" id="name" placeholder="Name" name="name" required>
          <div class="invalid-feedback">
              Your name is required.
            </div>
          </div>
        </div>

        <% for (let i = 0; i < 5; i++) { %>
          <div class="col-12">
            <label for="symptom<%= i + 1 %>" class="form-label">Symptom <%= i + 1 %></label>
            <select class="form-select" id="symptom<%= i + 1 %>" name="symptom<%= i + 1 %>" required>
              <option value="">Choose...</option>
              <% for (let j = 0; j < symptoms.length; j++) { %>
                <option value="<%= symptoms[j] %>"><%= symptoms[j] %></option>
              <% } %>
            </select>
          </div>
        <% } %>


        <div class="col-12">
          <button type="submit" class="btn" >Predict</button>
                       
        </div>
      </div>
    </form>
    </div>
  

